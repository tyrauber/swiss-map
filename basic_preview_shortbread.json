{
  "version": 8,
  "name": "Basic preview",
  "sources": {
    "shortbread": {
      "type": "vector",
      "url": "https://wipfli.github.io/swiss-map/output.json"
    }
  },
  "glyphs": "https://tiles.ballometer.io/fonts/{fontstack}/{range}.pbf",
  "layers": [
    {
      "id": "background",
      "type": "background",
      "layout": {"visibility": "visible"},
      "paint": {"background-color": "#f8f9fa"}
    },
    {
      "id": "landuse-residential",
      "type": "fill",
      "source": "shortbread",
      "source-layer": "land",
      "maxzoom": 14,
      "filter": [
        "all",
        ["==", "$type", "Polygon"],
        ["in", "kind", "residential", "industrial", "cemetery", "park", "farmyard", "commercial"]
      ],
      "layout": {"visibility": "visible"},
      "paint": {"fill-color": "#dee0e4"}
    },
    {
      "id": "landcover_grass",
      "type": "fill",
      "source": "shortbread",
      "source-layer": "land",
      "filter": ["==", "kind", "meadow"],
      "layout": {"visibility": "visible"},
      "paint": {"fill-color": "#f8f9fa"}
    },
    {
      "id": "landcover_wood",
      "type": "fill",
      "source": "shortbread",
      "source-layer": "land",
      "filter": ["in", "kind", "wood", "orchard"],
      "layout": {"visibility": "visible"},
      "paint": {"fill-color": "#ceead6"}
    },
    {
      "id": "water",
      "type": "fill",
      "source": "shortbread",
      "source-layer": "water_polygons",
      "filter": ["all", ["==", "$type", "Polygon"], ["==", "kind", "water"]],
      "layout": {"visibility": "visible"},
      "paint": {"fill-color": "hsl(205, 56%, 73%)"}
    },
    {
      "id": "waterway",
      "type": "line",
      "source": "shortbread",
      "source-layer": "water_lines",
      "filter": ["==", "$type", "LineString"],
      "layout": {"visibility": "visible"},
      "paint": {"line-color": "hsl(205, 56%, 73%)"}
    },
    {
      "id": "tunnel-service-track-casing",
      "type": "line",
      "source": "shortbread",
      "source-layer": "streets",
      "filter": [
        "all",
        ["==", "tunnel", true],
        ["in", "kind", "service", "track"]
      ],
      "layout": {"line-join": "round", "visibility": "visible"},
      "paint": {
        "line-color": "#cfcdca",
        "line-dasharray": [0.5, 0.25],
        "line-width": {"base": 1.2, "stops": [[15, 1], [16, 4], [20, 11]]}
      }
    },
    {
      "id": "tunnel-minor-casing",
