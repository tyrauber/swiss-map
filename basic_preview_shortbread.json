{
  "version": 8,
  "name": "Basic preview",
  "sources": {
    "openmaptiles": {
      "type": "vector",
      "url": "https://tiles.ballometer.io/data/v3.json"
    },
    "shortbread": {
      "type": "vector",
      "tiles": ["pmtiles://https://wipfli.github.io/swiss-map/output.pmtiles/{z}/{x}/{y}"],
      "maxzoom": 14
    }
  },
  "glyphs": "https://tiles.ballometer.io/fonts/{fontstack}/{range}.pbf",
  "layers": [
    {
      "id": "background",
      "type": "background",
      "layout": {"visibility": "visible"},
      "paint": {"background-color": "rgba(240, 244, 247, 1)"}
    },
    {
      "id": "landuse-residential",
      "type": "fill",
      "source": "shortbread",
      "source-layer": "land",
      "filter": [
        "all",
        ["==", "$type", "Polygon"],
        ["in", "kind", "residential", "industrial", "cemetery", "park"]
      ],
      "layout": {"visibility": "visible"},
      "paint": {"fill-color": "hsl(47, 13%, 86%)", "fill-opacity": 0.7}
    },
    {
      "id": "landcover_grass",
      "type": "fill",
      "source": "shortbread",
      "source-layer": "land",
      "filter": ["==", "kind", "meadow"],
      "layout": {"visibility": "visible"},
      "paint": {"fill-color": "rgba(240, 244, 247, 1)", "fill-opacity": 0.45}
    },
    {
      "id": "landcover_wood",
      "type": "fill",
      "source": "shortbread",
      "source-layer": "land",
      "filter": ["==", "kind", "wood"],
      "layout": {"visibility": "visible"},
      "paint": {"fill-color": "rgba(195, 222, 179, 1)", "fill-opacity": 0.5}
    },
    {
      "id": "water",
      "type": "fill",
      "source": "shortbread",
      "source-layer": "water_polygons",
      "filter": ["all", ["==", "$type", "Polygon"], ["==", "kind", "water"]],
      "layout": {"visibility": "visible"},
      "paint": {"fill-color": "hsl(205, 56%, 73%)"}
    },
    {
      "id": "waterway",
      "type": "line",
      "source": "shortbread",
      "source-layer": "water_lines",
      "filter": ["==", "$type", "LineString"],
      "layout": {"visibility": "visible"},
      "paint": {"line-color": "hsl(205, 56%, 73%)"}
    },
    {
      "id": "water_intermittent",
      "type": "fill",
      "source": "openmaptiles",
      "source-layer": "water",
      "filter": ["all", ["==", "$type", "Polygon"], ["==", "intermittent", 1]],
      "layout": {"visibility": "none"},
      "paint": {"fill-color": "hsl(205, 56%, 73%)", "fill-opacity": 0.7}
    },
    {
      "id": "landcover-ice-shelf",
      "type": "fill",
      "source": "openmaptiles",
      "source-layer": "landcover",
      "filter": ["==", "subclass", "ice_shelf"],
      "layout": {"visibility": "none"},
      "paint": {"fill-color": "hsl(47, 26%, 88%)", "fill-opacity": 0.8}
    },
    {
      "id": "landcover-glacier",
      "type": "fill",
      "source": "openmaptiles",
      "source-layer": "landcover",
      "filter": ["==", "subclass", "glacier"],
      "layout": {"visibility": "none"},
      "paint": {
        "fill-color": "hsl(47, 22%, 94%)",
        "fill-opacity": {"base": 1, "stops": [[0, 1], [8, 0.5]]}
      }
    },
    {
      "id": "landcover_sand",
      "type": "fill",
      "metadata": {},
      "source": "openmaptiles",
      "source-layer": "landcover",
      "filter": ["all", ["in", "kind", "sand"]],
      "layout": {"visibility": "none"},
      "paint": {
        "fill-antialias": false,
        "fill-color": "rgba(232, 214, 38, 1)",
        "fill-opacity": 0.3
      }
    },
    {
      "id": "landuse",
      "type": "fill",
      "source": "openmaptiles",
      "source-layer": "landuse",
      "filter": ["==", "kind", "agriculture"],
      "layout": {"visibility": "none"},
      "paint": {"fill-color": "rgba(240, 244, 247, 1)"}
    },
    {
      "id": "landuse_overlay_national_park",
      "type": "fill",
      "source": "openmaptiles",
      "source-layer": "landcover",
      "filter": ["==", "kind", "national_park"],
      "layout": {"visibility": "none"},
      "paint": {
        "fill-color": "#E1EBB0",
        "fill-opacity": {"base": 1, "stops": [[5, 0], [9, 0.75]]}
      }
    },
    {
      "id": "tunnel-service-track-casing",
      "type": "line",
      "source": "shortbread",
      "source-layer": "streets",
      "filter": [
        "all",
        ["==", "tunnel", true],
        ["in", "kind", "service", "track"]
      ],
      "layout": {"line-join": "round", "visibility": "visible"},
      "paint": {
        "line-color": "#cfcdca",
        "line-dasharray": [0.5, 0.25],
        "line-width": {"base": 1.2, "stops": [[15, 1], [16, 4], [20, 11]]}
      }
    },
    {
      "id": "tunnel-minor-casing",
      "type": "line",
      "source": "shortbread",
      "source-layer": "streets",
      "filter": ["all", ["==", "tunnel", true], ["==", "kind", "minor"]],
      "layout": {"line-join": "round", "visibility": "visible"},
      "paint": {
        "line-color": "#cfcdca",
        "line-opacity": {"stops": [[12, 0], [12.5, 1]]},
        "line-width": {
          "base": 1.2,
          "stops": [[12, 0.5], [13, 1], [14, 4], [20, 15]]
        }
      }
    },
    {
      "id": "tunnel-secondary-tertiary-casing",
      "type": "line",
      "source": "shortbread",
      "source-layer": "streets",
      "filter": [
        "all",
        ["==", "tunnel", true],
        ["in", "kind", "secondary", "tertiary"]
      ],
      "layout": {"line-join": "round", "visibility": "visible"},
      "paint": {
        "line-color": "#e9ac77",
        "line-opacity": 1,
        "line-width": {"base": 1.2, "stops": [[8, 1.5], [20, 17]]}
      }
    },
    {
      "id": "tunnel-trunk-primary-casing",
      "type": "line",
      "source": "shortbread",
      "source-layer": "streets",
      "filter": [
        "all",
        ["==", "tunnel", true],
        ["in", "kind", "primary", "trunk"]
      ],
      "layout": {"line-join": "round", "visibility": "visible"},
      "paint": {
        "line-color": "#e9ac77",
        "line-width": {
          "base": 1.2,
          "stops": [[5, 0.4], [6, 0.6], [7, 1.5], [20, 22]]
        }
      }
    },
    {
      "id": "tunnel-motorway-casing",
      "type": "line",
      "source": "shortbread",
      "source-layer": "streets",
      "filter": [
        "all",
        ["==", "tunnel", true],
        ["==", "kind", "motorway"]
      ],
      "layout": {"line-join": "round", "visibility": "visible"},
      "paint": {
        "line-color": "#e9ac77",
        "line-dasharray": [0.5, 0.25],
        "line-width": {
          "base": 1.2,
          "stops": [[5, 0.4], [6, 0.6], [7, 1.5], [20, 22]]
        }
      }
    },
    {
      "id": "tunnel-path",
      "type": "line",
      "source": "shortbread",
      "source-layer": "streets",
      "filter": [
        "all",
        ["==", "$type", "LineString"],
        ["all", ["==", "tunnel", true], ["==", "kind", "path"]]
      ],
      "layout": {"visibility": "visible"},
      "paint": {
        "line-color": "#cba",
        "line-dasharray": [1.5, 0.75],
        "line-width": {"base": 1.2, "stops": [[15, 1.2], [20, 4]]}
      }
    },
    {
      "id": "tunnel-service-track",
      "type": "line",
      "source": "shortbread",
      "source-layer": "streets",
      "filter": [
        "all",
        ["==", "tunnel", true],
        ["in", "kind", "service", "track"]
      ],
      "layout": {"line-join": "round", "visibility": "visible"},
      "paint": {
        "line-color": "#fff",
        "line-width": {"base": 1.2, "stops": [[15.5, 0], [16, 2], [20, 7.5]]}
      }
    },
    {
      "id": "tunnel-minor",
      "type": "line",
      "source": "shortbread",
      "source-layer": "streets",
      "filter": [
        "all",
        ["==", "tunnel", true],
        ["==", "kind", "minor_road"]
      ],
      "layout": {"line-join": "round", "visibility": "visible"},
      "paint": {
        "line-color": "#fff",
        "line-opacity": 1,
        "line-width": {"base": 1.2, "stops": [[13.5, 0], [14, 2.5], [20, 11.5]]}
      }
    },
    {
      "id": "tunnel-secondary-tertiary",
      "type": "line",
      "source": "shortbread",
      "source-layer": "streets",
      "filter": [
        "all",
        ["==", "tunnel", true],
        ["in", "kind", "secondary", "tertiary"]
      ],
      "layout": {"line-join": "round", "visibility": "visible"},
      "paint": {
        "line-color": "#fff4c6",
        "line-width": {"base": 1.2, "stops": [[6.5, 0], [7, 0.5], [20, 10]]}
      }
    },
    {
      "id": "tunnel-trunk-primary",
      "type": "line",
      "source": "shortbread",
      "source-layer": "streets",
      "filter": [
        "all",
        ["==", "tunnel", true],
        ["in", "kind", "primary", "trunk"]
      ],
      "layout": {"line-join": "round", "visibility": "visible"},
      "paint": {
        "line-color": "#fff4c6",
        "line-width": {"base": 1.2, "stops": [[6.5, 0], [7, 0.5], [20, 18]]}
      }
    },
    {
      "id": "tunnel-motorway",
      "type": "line",
      "source": "shortbread",
      "source-layer": "streets",
      "filter": [
        "all",
        ["==", "tunnel", true],
        ["==", "kind", "motorway"]
      ],
      "layout": {"line-join": "round", "visibility": "visible"},
      "paint": {
        "line-color": "#ffdaa6",
        "line-width": {"base": 1.2, "stops": [[6.5, 0], [7, 0.5], [20, 18]]}
      }
    },
    {
      "id": "tunnel-railway",
      "type": "line",
      "source": "shortbread",
      "source-layer": "streets",
      "filter": ["all", ["==", "tunnel", true], ["==", "kind", "rail"]],
      "layout": {"visibility": "visible"},
      "paint": {
        "line-color": "#bbb",
        "line-width": {"base": 1.4, "stops": [[14, 0.4], [15, 0.75], [20, 2]]},
        "line-dasharray": [2, 2]
      }
    },
    {
      "id": "aeroway-taxiway-casing",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "aeroway",
      "minzoom": 12,
      "filter": ["all", ["in", "kind", "taxiway"]],
      "layout": {
        "line-cap": "round",
        "line-join": "round",
        "visibility": "none"
      },
      "paint": {
        "line-color": "rgba(153, 153, 153, 1)",
        "line-width": {"base": 1.5, "stops": [[11, 2], [17, 12]]},
        "line-opacity": 1
      }
    },
    {
      "id": "aeroway-runway-casing",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "aeroway",
      "minzoom": 12,
      "filter": ["all", ["in", "kind", "runway"]],
      "layout": {
        "line-cap": "round",
        "line-join": "round",
        "visibility": "none"
      },
      "paint": {
        "line-color": "rgba(153, 153, 153, 1)",
        "line-width": {"base": 1.5, "stops": [[11, 5], [17, 55]]},
        "line-opacity": 1
      }
    },
    {
      "id": "aeroway-area",
      "type": "fill",
      "source": "openmaptiles",
      "source-layer": "aeroway",
      "minzoom": 4,
      "filter": [
        "all",
        ["==", "$type", "Polygon"],
        ["in", "kind", "runway", "taxiway"]
      ],
      "layout": {"visibility": "none"},
      "paint": {
        "fill-opacity": {"base": 1, "stops": [[13, 0], [14, 1]]},
        "fill-color": "rgba(255, 255, 255, 1)"
      }
    },
    {
      "id": "aeroway-taxiway",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "aeroway",
      "minzoom": 4,
      "filter": [
        "all",
        ["in", "kind", "taxiway"],
        ["==", "$type", "LineString"]
      ],
      "layout": {
        "line-cap": "round",
        "line-join": "round",
        "visibility": "none"
      },
      "paint": {
        "line-color": "rgba(255, 255, 255, 1)",
        "line-width": {"base": 1.5, "stops": [[11, 1], [17, 10]]},
        "line-opacity": {"base": 1, "stops": [[11, 0], [12, 1]]}
      }
    },
    {
      "id": "aeroway-runway",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "aeroway",
      "minzoom": 4,
      "filter": [
        "all",
        ["in", "kind", "runway"],
        ["==", "$type", "LineString"]
      ],
      "layout": {
        "line-cap": "round",
        "line-join": "round",
        "visibility": "none"
      },
      "paint": {
        "line-color": "rgba(255, 255, 255, 1)",
        "line-width": {"base": 1.5, "stops": [[11, 4], [17, 50]]},
        "line-opacity": {"base": 1, "stops": [[11, 0], [12, 1]]}
      }
    },
    {
      "id": "highway-area",
      "type": "fill",
      "source": "openmaptiles",
      "source-layer": "transportation",
      "filter": ["==", "$type", "Polygon"],
      "layout": {"visibility": "none"},
      "paint": {
        "fill-color": "hsla(0, 0%, 89%, 0.56)",
        "fill-outline-color": "#cfcdca",
        "fill-opacity": 0.9,
        "fill-antialias": false
      }
    },
    {
      "id": "highway-motorway-link-casing",
      "type": "line",
      "source": "shortbread",
      "source-layer": "streets",
      "minzoom": 12,
      "filter": [
        "all",
        ["!=", "bridge", true], ["!=", "tunnel", true],
        ["==", "kind", "motorway_link"]
      ],
      "layout": {
        "line-cap": "round",
        "line-join": "round",
        "visibility": "visible"
      },
      "paint": {
        "line-color": "#e9ac77",
        "line-opacity": 1,
        "line-width": {
          "base": 1.2,
          "stops": [[12, 1], [13, 3], [14, 4], [20, 15]]
        }
      }
    },
    {
      "id": "highway-link-casing",
      "type": "line",
      "source": "shortbread",
      "source-layer": "streets",
      "minzoom": 13,
      "filter": [
        "all",
        ["!=", "bridge", true], ["!=", "tunnel", true],
        [
          "in",
          "class",
          "primary_link",
          "secondary_link",
          "tertiary_link",
          "trunk_link"
        ]
      ],
      "layout": {
        "line-cap": "round",
        "line-join": "round",
        "visibility": "visible"
      },
      "paint": {
        "line-color": "#e9ac77",
        "line-opacity": 1,
        "line-width": {
          "base": 1.2,
          "stops": [[12, 1], [13, 3], [14, 4], [20, 15]]
        }
      }
    },
    {
      "id": "highway-minor-casing",
      "type": "line",
      "source": "shortbread",
      "source-layer": "streets",
      "filter": [
        "all",
        ["==", "$type", "LineString"],
        [
          "all",
          ["!=", "tunnel", "true"],
          ["in", "kind", "minor", "service", "residential", "unclassified"]
        ]
      ],
      "layout": {
        "line-cap": "round",
        "line-join": "round",
        "visibility": "visible"
      },
      "paint": {
        "line-color": "#cfcdca",
        "line-opacity": {"stops": [[12, 0], [12.5, 1]]},
        "line-width": {
          "base": 1.2,
          "stops": [[12, 0.5], [13, 1], [14, 4], [20, 15]]
        }
      }
    },
    {
      "id": "highway-track-casing",
      "type": "line",
      "source": "shortbread",
      "source-layer": "streets",
      "filter": [
        "all",
        ["==", "$type", "LineString"],
        [
          "all",
          ["!=", "tunnel", "true"],
          ["in", "kind", "track"],
          ["==", "tracktype", "grade1"]
        ]
      ],
      "layout": {
        "line-cap": "round",
        "line-join": "round",
        "visibility": "visible"
      },
      "paint": {
        "line-color": "#cfcdca",
        "line-opacity": {"stops": [[12, 0], [12.5, 1]]},
        "line-width": {
          "base": 1.2,
          "stops": [[12, 0.5], [13, 1], [14, 4], [20, 15]]
        }
      }
    },
    {
      "id": "highway-secondary-tertiary-casing",
      "type": "line",
      "source": "shortbread",
      "source-layer": "streets",
      "filter": [
        "all",
        ["!=", "bridge", true], ["!=", "tunnel", true],
        ["in", "kind", "secondary", "tertiary"]
      ],
      "layout": {
        "line-cap": "round",
        "line-join": "round",
        "visibility": "visible"
      },
      "paint": {
        "line-color": "#e9ac77",
        "line-opacity": 1,
        "line-width": {"base": 1.2, "stops": [[8, 1.5], [20, 17]]}
      }
    },
    {
      "id": "highway-primary-casing",
      "type": "line",
      "source": "shortbread",
      "source-layer": "streets",
      "minzoom": 5,
      "filter": [
        "all",
        ["!=", "bridge", true], ["!=", "tunnel", true],
        ["in", "kind", "primary"]
      ],
      "layout": {
        "line-cap": "round",
        "line-join": "round",
        "visibility": "visible"
      },
      "paint": {
        "line-color": "#e9ac77",
        "line-opacity": {"stops": [[7, 0], [8, 1]]},
        "line-width": {
          "base": 1.2,
          "stops": [[7, 0], [8, 0.6], [9, 1.5], [20, 22]]
        }
      }
    },
    {
      "id": "highway-trunk-casing",
      "type": "line",
      "source": "shortbread",
      "source-layer": "streets",
      "minzoom": 5,
      "filter": [
        "all",
        ["!=", "bridge", true], ["!=", "tunnel", true],
        ["in", "kind", "trunk"]
      ],
      "layout": {
        "line-cap": "round",
        "line-join": "round",
        "visibility": "visible"
      },
      "paint": {
        "line-color": "#e9ac77",
        "line-opacity": {"stops": [[5, 0], [6, 1]]},
        "line-width": {
          "base": 1.2,
          "stops": [[5, 0], [6, 0.6], [7, 1.5], [20, 22]]
        }
      }
    },
    {
      "id": "highway-motorway-casing",
      "type": "line",
      "source": "shortbread",
      "source-layer": "streets",
      "minzoom": 4,
      "filter": [
        "all",
        ["!=", "bridge", true], ["!=", "tunnel", true],
        ["==", "kind", "motorway"]
      ],
      "layout": {
        "line-cap": "round",
        "line-join": "round",
        "visibility": "visible"
      },
      "paint": {
        "line-color": "#e9ac77",
        "line-width": {
          "base": 1.2,
          "stops": [[4, 0], [5, 0.4], [6, 0.6], [7, 1.5], [20, 22]]
        },
        "line-opacity": {"stops": [[4, 0], [5, 1]]}
      }
    },
    {
      "id": "highway-path",
      "type": "line",
      "source": "shortbread",
      "source-layer": "streets",
      "filter": [
        "all",
        ["==", "$type", "LineString"],
        ["all", ["!=", "bridge", true], ["!=", "tunnel", true], ["in", "kind", "path", "footway", "steps"]]
      ],
      "layout": {"visibility": "visible"},
      "paint": {
        "line-color": "#cba",
        "line-dasharray": [1.5, 0.75],
        "line-width": {"base": 1.2, "stops": [[15, 1.2], [20, 4]]}
      }
    },
    {
      "id": "highway-motorway-link",
      "type": "line",
      "source": "shortbread",
      "source-layer": "streets",
      "minzoom": 12,
      "filter": [
        "all",
        ["!=", "bridge", true], ["!=", "tunnel", true],
        ["==", "kind", "motorway_link"]
      ],
      "layout": {
        "line-cap": "round",
        "line-join": "round",
        "visibility": "visible"
      },
      "paint": {
        "line-color": "#fc8",
        "line-width": {
          "base": 1.2,
          "stops": [[12.5, 0], [13, 1.5], [14, 2.5], [20, 11.5]]
        }
      }
    },
    {
      "id": "highway-link",
      "type": "line",
      "source": "shortbread",
      "source-layer": "streets",
      "minzoom": 13,
      "filter": [
        "all",
        ["!=", "bridge", true], ["!=", "tunnel", true],
        [
          "in",
          "class",
          "primary_link",
          "secondary_link",
          "tertiary_link",
          "trunk_link"
        ]
      ],
      "layout": {
        "line-cap": "round",
        "line-join": "round",
        "visibility": "visible"
      },
      "paint": {
        "line-color": "#fea",
        "line-width": {
          "base": 1.2,
          "stops": [[12.5, 0], [13, 1.5], [14, 2.5], [20, 11.5]]
        }
      }
    },
    {
      "id": "highway-minor",
      "type": "line",
      "source": "shortbread",
      "source-layer": "streets",
      "filter": [
        "all",
        ["==", "$type", "LineString"],
        [
          "all",
          ["!=", "tunnel", "true"],
          ["in", "kind", "minor", "service", "residential", "unclassified"]
        ]
      ],
      "layout": {
        "line-cap": "round",
        "line-join": "round",
        "visibility": "visible"
      },
      "paint": {
